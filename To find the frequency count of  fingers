#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    int r=n%8;
    if(r==1)
        printf("Thumb");
    else if(r==2 || r==8 || r==0)
        printf("index");
    else if(r==3 || r==7)
        printf("middle");
    else if(r==4 || r==6)
        printf("ring");
    else if(r==5)
        printf("little");
    return 0;
}
