#include<stdio.h>
int main()
{
    int n,i,j;
    printf("n=");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for(j=i;j>=1;j--)
        {
            printf("%d",j);
        }
        for(j=2;j<=n-i+1;j++)
        {
            printf("%d",j);
        }
    printf("\n");
    }
    return 0;
}

/*output
1234567
2123456
3212345
4321234
5432123
6543212
7654321 */

