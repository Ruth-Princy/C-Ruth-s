#include<stdio.h>
int main()
{
    int n,i,j,target;
    scanf("%d",&n);

    int arr[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    scanf("%d",&target);
    for(i=0;i<n;i++)
    {
        for(j=i+1;j<n;j++)
        {
            //k=arr[i]+arr[j];
            if(arr[i]+arr[j]==target)
          {
             printf("true");
             return 0;
          }
        }
    }
    printf("false");
}
